    {
        "id": "00d50bba346286c5",
        "type": "ui_template",
        "z": "f21a9ceb25b16cae",
        "group": "grp_dashboard",
        "name": "Grid de Localiza√ß√£o",
        "order": 5,
        "width": 12,
        "height": 8,
        "format": "<style>\n#gridWrapper {\n  display: grid;\n  grid-template-columns: repeat(10, 1fr);\n  grid-template-rows: repeat(10, 1fr);\n  width: 100%;\n  aspect-ratio: 1/1;\n  border: 2px solid #333;\n}\n.cell {\n  border: 1px solid #aaa;\n  background: #f2f2f2;\n  position: relative;\n}\n.cell.moto::after {\n  content: '\\1F3CD'; /* emoji moto */\n  position: absolute;\n  inset: 0;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 1.5em;\n}\n</style>\n\n<div id=\"gridWrapper\"></div>\n\n<script>\n(function(){\n  const grid = document.getElementById('gridWrapper');\n  for(let i=0;i<100;i++){\n    const cell = document.createElement('div');\n    cell.className = 'cell';\n    grid.appendChild(cell);\n  }\n\n  function updateGrid(lat, lng){\n    const idx = Math.floor(Math.random()*100); // simula√ß√£o\n    document.querySelectorAll('.cell').forEach(c=>c.classList.remove('moto'));\n    grid.children[idx].classList.add('moto');\n  }\n\n  window.addEventListener('nodeRedGridPayload', e => {\n    const {lat, lng} = e.detail;\n    updateGrid(lat,lng);\n  });\n})();\n</script>",
        "storeOutMessages": false,
        "fwdInMessages": false,
