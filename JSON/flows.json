    {
        "id": "1e146cfdcd98b6a6",
        "type": "ui_template",
        "z": "f21a9ceb25b16cae",
        "group": "grp_dashboard",
        "name": "Grid Sim: entrada (oculto)",
        "order": 99,
        "width": 0,
        "height": 0,
        "format": "<!-- template proxy: envia msg.payload para o grid vis√≠vel -->\n<script>\n(function(scope){\n  scope.$watch('msg', function(m){\n    if(!m) return;\n    // reencaminha para o template real via evento\n    const event = new CustomEvent('nodeRedGridPayload',{detail:m.payload});\n    window.dispatchEvent(event);\n  });\n})(scope);\n</script>",
        "storeOutMessages": true,
